{
    "name": "adventofcode",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": ">= 14.15.0"
    },
    "scripts": {
        "prettier:fix": "prettier --write \"src/**/*.ts\"",
        "prettier:check": "prettier --check \"src/**/*.ts\"",
        "ts": "tsc --noEmit",
        "test": "yarn run ts && yarn run lint && yarn run prettier:check",
        "lint": "eslint --max-warnings=0 src"
    },
    "dependencies": {
        "prettier": "2.2.0",
        "ts-node": "9.0.0",
        "typescript": "4.1.2"
    },
    "devDependencies": {
        "@types/node": "14.14.7",
        "@types/prettier": "2.1.5",
        "@typescript-eslint/eslint-plugin": "4.8.1",
        "@typescript-eslint/parser": "4.8.1",
        "eslint": "7.13.0",
        "eslint-config-prettier": "6.15.0"
    }
}
