{
    "name": "adventofcode",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": ">= 12.13.0"
    },
    "scripts": {
        "prettier:fix": "prettier --write \"src/**/*.ts\"",
        "prettier:check": "prettier --check \"src/**/*.ts\"",
        "ts": "tsc --noEmit",
        "test": "yarn run ts && yarn run lint && yarn run prettier:check",
        "lint": "eslint src"
    },
    "dependencies": {
        "prettier": "2.1.2",
        "ts-node": "9.0.0",
        "typescript": "4.0.3"
    },
    "devDependencies": {
        "@types/node": "12.12.66",
        "@types/prettier": "2.1.1",
        "@typescript-eslint/eslint-plugin": "4.4.0",
        "@typescript-eslint/parser": "4.4.0",
        "eslint": "7.10.0",
        "eslint-config-prettier": "6.12.0"
    }
}
