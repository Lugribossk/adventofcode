{
    "name": "adventofcode",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": ">= 18.16.0",
        "npm": ">= 9.5.1"
    },
    "volta": {
        "node": "18.16.0",
        "npm": "9.5.1"
    },
    "scripts": {
        "test": "npm run ts && npm run eslint:check && npm run prettier:check && npx type-coverage",
        "fix": "npm run eslint:fix && npm run prettier:fix",
        "prettier:fix": "prettier --write --cache \"src/**/*.ts\" \".github/**/*.yml\" .eslintrc.js package.json tsconfig.json",
        "prettier:check": "prettier --check --cache \"src/**/*.ts\" \".github/**/*.yml\" .eslintrc.js package.json tsconfig.json",
        "ts": "tsc --noEmit",
        "eslint:fix": "eslint --fix --cache --cache-location ./target/.eslintcache src",
        "eslint:check": "eslint --max-warnings=0 --report-unused-disable-directives --cache --cache-location ./target/.eslintcache src"
    },
    "typeCoverage": {
        "atLeast": 100,
        "detail": true,
        "cache": true,
        "cacheDirectory": "target/.type-coverage"
    },
    "dependencies": {
        "@swc/core": "1.3.63",
        "prettier": "2.8.8",
        "ts-node": "10.9.1",
        "typescript": "5.0.2"
    },
    "devDependencies": {
        "@types/node": "18.16.18",
        "@types/prettier": "2.7.3",
        "@typescript-eslint/eslint-plugin": "5.59.11",
        "@typescript-eslint/parser": "5.59.11",
        "eslint": "8.42.0",
        "eslint-config-prettier": "8.8.0",
        "type-coverage": "2.26.0"
    }
}
