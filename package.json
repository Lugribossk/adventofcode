{
    "name": "adventofcode",
    "version": "0.0.1",
    "private": true,
    "engines": {
        "node": ">= 12.13.0"
    },
    "scripts": {
        "prettier:fix": "prettier --write \"src/**/*.ts\"",
        "prettier:check": "prettier --check \"src/**/*.ts\"",
        "ts": "tsc --noEmit",
        "test": "yarn run ts && yarn run lint && yarn run prettier:check",
        "lint": "eslint src/**/*.ts"
    },
    "dependencies": {
        "prettier": "1.19.1",
        "ts-node": "8.5.4",
        "typescript": "3.7.3"
    },
    "devDependencies": {
        "@types/node": "12.12.21",
        "@types/prettier": "1.19.0",
        "@typescript-eslint/eslint-plugin": "2.14.0",
        "@typescript-eslint/parser": "2.14.0",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.9.0"
    }
}
